_G.AutoTap = true

while _G.AutoTap == true do
game:GetService("ReplicatedStorage").Remotes.Tap:FireServer()
wait(.01)
end

game:GetService("StarterGui"):SetCore("SendNotification",{
    Title = "Tapping Legends X Script:";
    Text = "Successfully Loaded Autoclicker!";
    Duration = 3
})

--> simple script!
