_G.AutoTap = true

game:GetService("StarterGui"):SetCore("SendNotification",{
    Title = "Tapping Legends X:";
    Text = "Loaded AutoTapper!";
    Duration = 3
})

while _G.AutoTap == true do
game:GetService("ReplicatedStorage").Remotes.Tap:FireServer()
wait(.01)
end

--> simple script!
